{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier", "tslint-config-airbnb"],
  "rules": {
    "ordered-imports": true,
    "ban": false,
    "class-name": true,
    "curly": true,
    "indent": [true, "spaces"],
    "interface-name": true,
    "max-line-length": [true, 120],
    "no-arg": true,
    "no-console": [true, "debug", "info"],
    "no-var-keyword": true,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "prefer-object-spread": true,
    "no-var-requires": false,
    "no-implicit-dependencies": false,
    "jsx-no-lambda": false,
    "member-access": false,
    "no-submodule-imports": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "one-line": [true, "check-catch", "check-else", "check-open-brace", "check-whitespace"],
    "radix": true,
    "switch-default": true,
    "trailing-comma": [true],
    "triple-equals": [true, "allow-null-check"],
    "arrow-parens": [false],
    "typedef": [true, "parameter", "property-declaration"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "import-name": false,
    "object-literal-sort-keys": false,
    "prefer-array-literal": false,
    "ter-arrow-parens": false,
    "no-empty-interface": false,
    "align": [true, "parameters", "statements", "members", "elements"]
  }
}
